PRODUCT_NAME = Messenger Helper

LD_RUNPATH_SEARCH_PATHS = $(inherited) @executable_path/../../../../Frameworks
INFOPLIST_FILE = Messenger Helper/Info.plist

SYMROOT = $(SRCROOT)/build

